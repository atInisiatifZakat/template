{
    "name": "inisiatif/template",
    "description": "Template package using in Inisiatif Zakat Indonesia",
    "license": "proprietary",
    "type": "library",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/inisiatif/"
        },
        {
            "packagist.org": false
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "barryvdh/laravel-snappy": "^1.0",
        "setasign/fpdf": "1.8.*",
        "setasign/fpdi": "2.5",
        "setasign/fpdi-protection": "2.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Inisiatif\\Package\\Template\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inisiatif\\Package\\Template\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
